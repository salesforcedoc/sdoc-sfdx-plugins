{
  "name": "sdoc-sfdx-plugins",
  "description": "Helpful plugin commands for Salesforce CLI built by Andrew Sim",
  "version": "0.0.0",
  "author": "salesforcedoc @salesforcedoc",
  "bugs": "https://github.com/salesforcedoc/sdoc-sfdx-plugins/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^1.4.1",
    "@salesforce/core": "^1.3.2",
    "json2csv": "^4.3.5",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "~3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforcedoc/sdoc-sfdx-plugins",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "sdoc:object": {
        "description": "object commands."
      },
      "sdoc:mdapi": {
        "description": "mdapi commands."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "salesforcedoc/sdoc-sfdx-plugins",
  "scripts": {
    "clean": "rm -f oclif.manifest.json",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "echo 'tslint -p test -t stylish'",
    "prepare": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}